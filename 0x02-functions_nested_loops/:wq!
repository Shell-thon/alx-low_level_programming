#include "main.h"

/**
 * void print_alphabet_x10: main function
 *
 * Descrition: prints 10 times the alphabet, in lowercase, followed by a new line.
 */
void print_alphabet_x10(void)
{
	char alpha = 'a';
	int i =0;

	while (i < 10)
	{
		for (alpha = 'a'; ch <= 'z'; ch++)
		{
			_putchar(alpha);
		}
		i++;
		_putchar(10);
	}
}
